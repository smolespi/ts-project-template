{
  "name": "ts-project-template",
  "version": "0.0.1",
  "description": "Project description",
  "author": "smolespi <espi@lesbian.codes> (https://lesbian.codes)",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/smolespi/ts-project-template",
  "main": "dist/src/index.ts",
  "bugs": {
    "url": "https://github.com/smolespi/ts-project-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smolespi/ts-project-template.git"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "format": "beautify-proj -i . -o ../ -c .jsbeautifyrc -e ./node_modules",
    "start": "tsc && cross-env NODE_ENV=production node dist/src/index.js",
    "test": "eslint ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "beautify-proj": "^1.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
