{
  "private": "true",
  "name": "ts-project-template",
  "version": "0.0.1",
  "description": "Project description",
  "author": "Espi <espi@riseup.net> (https://espi.me)",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/smolespi/ts-project-template",
  "main": "dist/src/index.js",
  "bugs": {
    "url": "https://github.com/smolespi/ts-project-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smolespi/ts-project-template.git"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run nodemon",
    "format": "cross-env prettier --config .prettierrc 'src/**/*.{css,htm,html,js,json,ts,tsx}' --write",
    "lint": "eslint --fix .",
    "nodemon": "cross-env nodemon --ext 'json,ts,sass,scss' --watch 'src' --delay 5 --exec 'cross-env NODE_ENV=development ts-node --files ./src/index.ts'",
    "start": "npm run build && cross-env NODE_ENV=production node dist/src/index.js",
    "test": "tsc && eslint ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
