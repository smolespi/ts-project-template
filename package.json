{
  "private": "true",
  "name": "@sysdotini/ts-project-template",
  "version": "0.0.1",
  "description": "Project description",
  "author": "sysdotini <espi@riseup.net> (https://espi.me)",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/sysdotini/ts-project-template",
  "main": "dist/index.js",
  "bugs": {
    "url": "https://github.com/sysdotini/ts-project-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysdotini/ts-project-template.git"
  },
  "engines": {
    "npm": ">=6.9.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run nodemon",
    "format": "cross-env prettier --config .prettierrc 'src/**/*.{css,ejs,liquid,js,json,sass,scss,ts,tsx}' --write",
    "lint": "eslint --fix .",
    "nodemon": "cross-env nodemon --ext 'ts' --watch 'src' --delay 5 --exec 'cross-env NODE_ENV=development ts-node --files ./src/index.ts'",
    "prebuild": "cross-env rimraf ./dist/",
    "prepublishOnly": "npm run test && npm run build",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "test": "tsc && eslint ."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
